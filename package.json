  "main": "./lib/pacopack",
  "exports": {
    "import": "./lib/pacopack.mjs",
    "require": "./lib/pacopack.js"
  },
  "files": [
    "lib/",
    "types/"
  ],
  "scripts": {
    "watch": "npx tsc --watch --inlineSourcemap",
    "build": "npx rollup -c"
  },
